{
  "plugins": [
    "promise"
  ],
  "extends": [
    "airbnb",
    "plugin:promise/recommended"
  ],
  "rules": {
    "no-extra-semi": "warn",
    "semi": 2,
    "space-before-function-paren": 0,
    "no-console": 0,
    "max-len": [
      "error",
      150
    ],
    "no-trailing-spaces": [
      "error",
      "skipBlankLines": true,
    ]
  },
  "env": {
    "mocha": "true"
  }
}